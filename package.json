  "scripts": {
    "postinstall": "./tasks/privateDependencies.sh",
    "lint": "eslint $(find src/app -type f -name '*.js')  --quiet",
    "e2e": "protractor protractor_conf.js",
    "pretest": "del ./build && cross-env NODE_ENV=test webpack --progress",
    "test": "cross-env NODE_ENV=test karma start test/karma.conf.js",
    "testwatch": "npm test -- --auto-watch --no-single-run",
    "postbuild:i18n": "npm run i18n",
    "i18n:check": "tasks/noContext",
    "preversion": "rm package-lock.json || echo 'No useless package-lock to remove'",
    "postversion": "git push --tags",
    "start": "cross-env NODE_ENV=dev tasks/setupConfig.js",
    "poststart": "cross-env NODE_ENV=dev tasks/buildVendor.js && cross-env NODE_ENV=dev && webpack-dev-server --hot --progress",
    "dist": "cross-env NODE_ENV=dist tasks/buildVendor.js && cross-env NODE_ENV=dist webpack --progress",
    "deploy": "del ./dist && cross-env NODE_ENV=dist tasks/deploy.js",
    "postdist": "tasks/vendorCheckSum.sh",
    "i18n:extract": "tasks/extractI18N.sh",
    "i18n:compile": "tasks/compileI18N.sh",
    "i18n:build": "npm run i18n:extract && npm run i18n:compile",
    "posti18n:build": "./tasks/createI18nCache.js && ./tasks/commitI18n.sh",
    "pretty": "prettier --arrow-parens=always --single-quote --tab-width=4 --print-width=150 --no-prose-wrap --write $(find src/app -type f -name '*.js')",
    "precommit": "lint-staged",
    "create": "appComponent"
  },
  "lint-staged": {
    "*.js": [
      "prettier --arrow-parens=always --single-quote --tab-width=4 --print-width=150 --no-prose-wrap --write",
      "git add"
    ]
  },
