  "scripts": {
    "postinstall": "./tasks/checkDependencies.js",
    "lint": "eslint src/app src/helpers --ext .js --quiet --cache",
    "e2e": "./tasks/lazyInstallVendor e2e && cypress run",
    "pretest": "./tasks/lazyInstallVendor test && del ./build && cross-env NODE_ENV=test npm run config",
    "test": "cross-env NODE_ENV=test karma start test/karma.conf.js",
    "testwatch": "npm test -- --auto-watch --no-single-run",
    "postbuild:i18n": "npm run i18n",
    "i18n:check": "tasks/noContext",
    "preversion": "rm package-lock.json || echo 'No useless package-lock to remove'",
    "postversion": "git push --tags",
    "config": "cross-env NODE_ENV=dev NODE_ENV_MODE=config tasks/setupConfig.js",
    "start": "cross-env NODE_ENV=dev tasks/setupConfig.js",
    "poststart": "npm run dev",
    "start:raw": "cross-env NODE_ENV=dev tasks/setupConfig.js",
    "poststart:raw": "npm run dev -- --no-transpile",
    "dev": "cross-env NODE_ENV=dev && webpack-dev-server --hot --progress",
    "dist": "cross-env NODE_ENV=dist webpack --progress",
    "build": "cross-env NODE_ENV=dev tasks/setupConfig.js",
    "postbuild": "cross-env NODE_ENV=dev && webpack --progress",
    "deploy": "cross-env NODE_ENV=dist tasks/deploy.js",
    "deployProd": "npm run deploy -- --branch=deploy-prod-a --flow=many",
    "build:ci": "cross-env NODE_ENV=dist NODE_ENV_DIST=ci tasks/deploy.js",
    "releaser:extract": "releaser --config releaser.config.js --token $RELEASER_GH_TOKEN",
    "releaser:unshift": "./tasks/unshiftReleaser.sh",
    "i18n:update": "tasks/updateTranslations.sh",
    "i18n:extract": "tasks/extractI18N.sh",
    "i18n:compile": "tasks/compileI18n.js",
    "i18n:build": "npm run i18n:extract && npm run i18n:update && npm run i18n:compile",
    "posti18n:build": "./tasks/createI18nCache.js && ./tasks/commitI18n.sh",
    "pretty": "prettier --write $(find src/app -type f -name '*.js')",
    "create": "appComponent",
    "create:test": "appComponent --test",
    "cypress:open": "./tasks/lazyInstallVendor e2e && cypress open"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "env/env.json": [
      "tasks/checkEnv.js"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "version": "3.15.7",
  "husky": {
    "hooks": {
      "pre-commit": "./tasks/metrics.sh"
    }
