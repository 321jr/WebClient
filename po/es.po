#: src/app/templates/modals/hotkey.tpl.html:12
msgctxt "Info"
msgid "<kbd>?</kbd> Opens the <strong>help modal</strong>."
msgstr "<kbd>?</kbd> Abre la <strong>ventana de ayuda</strong>."

#: src/app/templates/modals/hotkey.tpl.html:33
msgctxt "Info"
msgid "<kbd>.</kbd> Marks or unmarks the message/conversation as <strong>starred</strong>."
msgstr "<kbd>.</kbd> Marca o desmarca el mensaje/conversación como <strong>favorito</strong>."

#: src/app/templates/modals/hotkey.tpl.html:28
msgctxt "Info"
msgid "<kbd>*</kbd> then <kbd>a</kbd> <strong>Select all</strong> conversations."
msgstr "<kbd>*</kbd> y luego <kbd>a</kbd> <strong>Selecciona todas</strong> las conversaciones."

#: src/app/templates/modals/hotkey.tpl.html:29
msgctxt "Info"
msgid "<kbd>*</kbd> then <kbd>n</kbd> <strong>Unselect all</strong> conversations."
msgstr "<kbd>*</kbd> y luego <kbd>n</kbd> <strong>De-selecciona todas</strong> las conversaciones."

#: src/app/templates/modals/hotkey.tpl.html:13
msgctxt "Info"
msgid "<kbd>/</kbd> Focus the <strong>search input</strong>."
msgstr "<kbd>/</kbd> Posicionarse <strong>en el campo de búsqueda</strong>."

#: src/app/templates/modals/hotkey.tpl.html:60
msgctxt "Info"
msgid "<kbd><i class=\"fa fa-long-arrow-down\"></i></kbd> Marks the <strong>next</strong> message/conversation."
msgstr "<kbd><i class=\"fa fa-long-arrow-down\"></i></kbd> Marca el <strong>siguiente</strong> mensaje/conversación."

#: src/app/templates/modals/hotkey.tpl.html:61
msgctxt "Info"
msgid "<kbd><i class=\"fa fa-long-arrow-left\"></i></kbd> Marks the last message."
msgstr "<kbd><i class=\"fa fa-long-arrow-left\"></i></kbd> Marca el último mensaje."

#: src/app/templates/modals/hotkey.tpl.html:62
msgctxt "Info"
msgid "<kbd><i class=\"fa fa-long-arrow-right\"></i></kbd> Unmarks the message."
msgstr "<kbd><i class=\"fa fa-long-arrow-right\"></i></kbd> Desmarca el mensaje."

#: src/app/templates/modals/hotkey.tpl.html:59
msgctxt "Info"
msgid "<kbd><i class=\"fa fa-long-arrow-up\"></i></kbd> Marks the <strong>previous</strong> message/conversation."
msgstr "<kbd><i class=\"fa fa-long-arrow-up\"></i></kbd> Elige <strong>previamente</strong> mensaje/conversación."

#: src/app/templates/modals/hotkey.tpl.html:36
msgctxt "Info"
msgid "<kbd>a</kbd> Moves the message/conversation to <strong>archive</strong>."
msgstr "<kbd>a</kbd> Mueve el mensaje/conversación al <strong>archivo</strong>."

#: src/app/templates/modals/hotkey.tpl.html:19
msgctxt "Info"
msgid "<kbd>c</kbd> Opens a new <strong>composer</strong>."
msgstr "<kbd>c</kbd> Abrir un nuevo <strong>creador</strong>."

#: src/app/templates/modals/hotkey.tpl.html:21
msgctxt "Info"
msgid "<kbd>command</kbd> + <kbd>enter</kbd> <strong>Sends</strong> the message."
msgstr "<kbd>comando</kbd> + <kbd>enter</kbd> <strong>Envía</strong> el mensaje."

#: src/app/templates/modals/hotkey.tpl.html:20
msgctxt "Info"
msgid "<kbd>ctrl</kbd> + <kbd>enter</kbd> <strong>Sends</strong> the message."
msgstr "<kbd>Ctrl</kbd> + <kbd>enter</kbd> <strong>Envía</strong> el mensaje."

#: src/app/templates/modals/hotkey.tpl.html:14
msgctxt "Info"
msgid "<kbd>enter</kbd> Confirms <strong>the active modal</strong>."
msgstr "<kbd>enter</kbd> Confirma <strong>la ventana activa</strong>."

#: src/app/templates/modals/hotkey.tpl.html:65
msgctxt "Info"
msgid "<kbd>enter</kbd> Opens the <strong>marked</strong> message/conversation."
msgstr "<kbd>entrar en</kbd> Abre el mensaje/conversación <strong>marcado</strong>."

#: src/app/templates/modals/hotkey.tpl.html:66
msgctxt "Info"
msgid "<kbd>escape</kbd> Back to the list."
msgstr "<kbd>escape</kbd> Volver a la lista."

#: src/app/templates/modals/hotkey.tpl.html:15
msgctxt "Info"
msgid "<kbd>escape</kbd> Closes <strong>the active modal</strong>."
msgstr "<kbd>escape</kbd> Cierra <strong>la ventana activa</strong>."

#: src/app/templates/modals/hotkey.tpl.html:22
msgctxt "Info"
msgid "<kbd>escape</kbd> Closes the active <strong>composer</strong>."
msgstr "<kbd>escape</kbd> Cierra el <strong>compositor</strong> activo."

#: src/app/templates/modals/hotkey.tpl.html:52
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>.</kbd> Goes to <strong>starred</strong>."
msgstr "<kbd>g</kbd> luego <kbd>.</kbd> Va a <strong>favoritos</strong>."

#: src/app/templates/modals/hotkey.tpl.html:53
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>a</kbd> Goes to <strong>archive</strong>."
msgstr "<kbd>g</kbd> luego <kbd>a</kbd> Va al <strong>archivo</strong>."

#: src/app/templates/modals/hotkey.tpl.html:50
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>d</kbd> Goes to <strong>drafts</strong>."
msgstr "<kbd>g</kbd> luego <kbd>d</kbd> Va a <strong>borradores</strong>."

#: src/app/templates/modals/hotkey.tpl.html:49
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>i</kbd> Goes to <strong>inbox</strong>."
msgstr "<kbd>g</kbd> luego <kbd>i</kbd> Va a <strong>bandeja de entrada</strong>."

#: src/app/templates/modals/hotkey.tpl.html:51
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>s</kbd> Goes to <strong>sent</strong>."
msgstr "<kbd>g</kbd> luego <kbd>s</kbd> Va a <strong>enviados</strong>."

#: src/app/templates/modals/hotkey.tpl.html:55
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>t</kbd> Goes to <strong>trash</strong>."
msgstr "<kbd>g</kbd> luego <kbd>t</kbd> Va a <strong>papelera</strong>."

#: src/app/templates/modals/hotkey.tpl.html:54
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>x</kbd> Goes to <strong>spam</strong>."
msgstr "<kbd>g</kbd> luego <kbd>x</kbd> Va a <strong>spam</strong>."

#: src/app/templates/modals/hotkey.tpl.html:34
msgctxt "Info"
msgid "<kbd>i</kbd> Moves the message/conversation to <strong>inbox</strong>."
msgstr "<kbd>i</kbd> Mueve el mensaje/conversación a la <strong>bandeja de entrada</strong>."

#: src/app/templates/modals/hotkey.tpl.html:64
msgctxt "Info"
msgid "<kbd>j</kbd> Displays the <strong>older</strong> conversation."
msgstr "<Kbd>j</kbd>Muestra la conversión <strong>antigua</strong>."

#: src/app/templates/modals/hotkey.tpl.html:63
msgctxt "Info"
msgid "<kbd>k</kbd> Displays the <strong>newer</strong> conversation."
msgstr "<kbd>k</kbd> muestra la conversación <strong>más reciente</strong>."

#: src/app/templates/modals/hotkey.tpl.html:31
msgctxt "Info"
msgid "<kbd>r</kbd> Marks the message/conversation as <strong>read</strong>."
msgstr "<kbd>r</kbd> Marca el mensaje/conversación como <strong>leído</strong>."

#: src/app/templates/modals/hotkey.tpl.html:37
msgctxt "Info"
msgid "<kbd>s</kbd> Moves the message/conversation to <strong>spam</strong>."
msgstr "<kbd>s</kbd> Mueve el mensaje/conversación a <strong>spam</strong>."

#: src/app/templates/modals/hotkey.tpl.html:42
msgctxt "Info"
msgid "<kbd>shift</kbd> + <kbd>a</kbd> Reply to all recipients for a message/conversation."
msgstr "<kbd>shift</kbd> + <kbd>a</kbd> Responder a todos los destinatarios de un mensaje/conversación."

#: src/app/templates/modals/hotkey.tpl.html:43
msgctxt "Info"
msgid "<kbd>shift</kbd> + <kbd>f</kbd> Forward a message/conversation."
msgstr "<kbd>Shift</kbd> + <kbd>F</kbd> reenviar un mensaje/conversación."

#: src/app/templates/modals/hotkey.tpl.html:41
msgctxt "Info"
msgid "<kbd>shift</kbd> + <kbd>r</kbd> Reply to a message/conversation."
msgstr "<kbd>Shift</kbd> + <kbd>F</kbd> reenviar un mensaje/conversación."

#: src/app/templates/modals/hotkey.tpl.html:35
msgctxt "Info"
msgid "<kbd>t</kbd> Moves the message/conversation to <strong>trash</strong>."
msgstr "<kbd>s</kbd> Mueve el mensaje/conversación a <strong>spam</strong>."

#: src/app/templates/modals/hotkey.tpl.html:32
msgctxt "Info"
msgid "<kbd>u</kbd> Marks the message/conversation as <strong>unread</strong>."
msgstr "<kbd>r</kbd> Marca el mensaje/conversación como <strong>leído</strong>."

#: src/app/templates/modals/hotkey.tpl.html:30
msgctxt "Info"
msgid "<kbd>x</kbd> Selects the message/conversation marked."
msgstr "<kbd>x</kbd> selecciona el mensaje/conversación marcada."

#: src/app/templates/views/signup.tpl.html:234
msgid "<p>Too many ProtonMail accounts have been created from your connection.</p>\n"
"                <p>Thus, we are requesting additional verification to ensure you are human and not a spam bot.</p>\n"
"                <p>Because Tor is frequently abused by spammers, this check may be triggered because of the Tor exit node you are using.</p>"
msgstr "<P> Se han creado demasiadas cuentas de ProtonMail desde su conexión. </ P>\n"
"                 <P> Por lo tanto, estamos solicitando verificación adicional para asegurar que eres humano y no un bot de spam. </ P>\n"
"                 <P> Debido a que los spammers abusan frecuentemente de Tor, esta comprobación puede ser activada debido al nodo de salida de Tor que está utilizando. </ P>"

